      "adaptiveIcon": {
        "foregroundImage": "./src/icon.png",
        "backgroundColor": "#6366F1"
      }
    },
    "splash": {
      "image": "./src/icon.png",
      "backgroundColor": "#6366F1",
      "resizeMode": "contain"
    },
    "extra": {
      "eas": {
        "projectId": "5f343e27-5b81-42fc-8220-302ce8233661"
      }
    }
  }
